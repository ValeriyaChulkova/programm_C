//Caмостоятельная работа 3 базовый вариант while 22

#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int N;
    bool isPrime = true;
    cout << 1: ";
    cin >> N;

    if (N <= 1) { 
        isPrime = false;
    } else {
        for (int i = 2; i <= sqrt(N); i++) { 
            if (N % i == 0) { 
                isPrime = false;
                break;
            }
        }
    }

    if (isPrime) {
        cout << "True";
    } else {
        cout << "False";
    }
    return 0;
}
