//Самостотельная работа 2 базовый вариант boolen 30
#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cin >> a >> b >> c;

    if (a == b && b == c) {
        cout << a << ", " << b << ", " << c << endl;
    } else {
        cout << a << ", " << b << ", " << c << endl;
    }

    return 0;
}
