//Сaмостоятеьная работа 1 базовый вариант integer 15
#include <iostream>
using namespace std;

int main() {
    int num;
    cout << ": ";
    cin >> num;

    int hundreds = num / 100;  
    int tens = num / 10 % 10;   
    int ones = num % 10;        

    int newNum = tens * 100 + hundreds * 10 + ones;   

    cout << ": " << newNum << endl;

    return 0;
}
